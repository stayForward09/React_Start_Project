{
  "name": "web-project-template",
  "version": "1.0.0",
  "description": "A template to quickstart react web projects.",
  "main": "app.js",
  "scripts": {
    "test": "(eslint ./src/**) && jest && webpack",
    "jest": "jest",
    "build": "NODE_ENV=production webpack",
    "dev": "webpack-dev-server --history-api-fallback --devtool eval --progress --colors --hot --content-base build",
    "lint": "eslint ./ -f stylish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arjunmehta/web-project-template.git"
  },
  "author": "Arjun Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arjunmehta/web-project-template/issues"
  },
  "homepage": "https://github.com/arjunmehta/web-project-template#readme",
  "dependencies": {
    "react-router": "1.0.0",
    "react": "0.14.3",
    "react-dom": "0.14.2",
    "history": "1.13.1"
  },
  "devDependencies": {
    "babel-core": "6.2.1",
    "babel-jest": "6.0.1",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": " 6.3.13",
    "copy-webpack-plugin": "0.2.0",
    "css-loader": "0.23.0",
    "eslint": "1.10.1",
    "eslint-plugin-react": "3.10.0",
    "file-loader": "0.8.4",
    "jest-cli": "0.8.0",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "react-addons-test-utils": "0.14.3",
    "style-loader": "0.13.0",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.12.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
